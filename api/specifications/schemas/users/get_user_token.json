{
    "title": "Get user token",
    "description": "Get the token with username and password",
    "type": "object",
    "$schema": "http://json-schema.org/draft-04/schema",
    "required": [
        "username",
        "email",
        "password"
    ],
    "properties": {
        "username": {
            "type": "string",
            "minLength": 2
        },
        "email": {
            "type": "string",
            "format": "email",
            "minLength": 3
        }
    }
}
