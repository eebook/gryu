{
    "title": "Get user token",
    "description": "Get the token with username and password",
    "type": "object",
    "$schema": "http://json-schema.org/draft-04/schema",
    "required": [
        "password"
    ],
    "properties": {
        "username": {
            "type": "string"
        },
        "email": {
            "type": "string",
            "format": "email"
        },
        "password": {
            "type": "string"
        }
    }
}
